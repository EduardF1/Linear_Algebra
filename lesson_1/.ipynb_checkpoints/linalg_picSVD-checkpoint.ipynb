    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from PIL import Image\n",
    "\n",
    "# import picture\n",
    "pic = Image.open('Einstein_tongue.jpg')\n",
    "\n",
    "# show the picture\n",
    "plt.imshow(pic)\n",
    "plt.show()\n",
    "\n",
    "# convert image to floating-point precision\n",
    "pic = np.array(pic)\n",
    "plt.imshow(pic,cmap='gray')\n",
    "plt.show()"
   ]
  }
